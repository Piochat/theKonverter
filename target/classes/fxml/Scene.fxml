<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/10.0.1" fx:controller="com.koke.thekonverter.FXMLController">
    <children>
        <Button fx:id="btnOpen" layoutX="521.0" layoutY="14.0" onAction="#open" prefWidth="60.0" text="Open" AnchorPane.rightAnchor="12.0" />
      <Button fx:id="btnUp" layoutX="536.0" layoutY="44.0" mnemonicParsing="false" onAction="#upDown" prefWidth="60.0" text="Up" AnchorPane.rightAnchor="12.0" />
      <Button fx:id="btnDown" layoutX="536.0" layoutY="74.0" mnemonicParsing="false" onAction="#upDown" prefWidth="60.0" text="Down" AnchorPane.rightAnchor="12.0" />
      <Button fx:id="btnDel" layoutX="536.0" layoutY="104.0" mnemonicParsing="false" onAction="#delete" prefWidth="60.0" text="Del" AnchorPane.rightAnchor="12.0" />
      <Button fx:id="btnClear" layoutX="528.0" layoutY="134.0" mnemonicParsing="false" onAction="#clear" prefWidth="60.0" text="Clear" AnchorPane.rightAnchor="12.0" />
      <Button fx:id="btnPdf" layoutX="528.0" layoutY="361.0" mnemonicParsing="false" onAction="#convertPdf" prefWidth="60.0" text="PDF" AnchorPane.rightAnchor="12.0" />
      <Button fx:id="btnCbz" layoutX="532.0" layoutY="331.0" mnemonicParsing="false" onAction="#convertCbz" prefWidth="60.0" text="CBZ" AnchorPane.rightAnchor="12.0" />
      <ListView fx:id="listView" layoutX="226.0" layoutY="17.0" onMouseClicked="#imagePicked" prefHeight="372.0" prefWidth="291.0" AnchorPane.topAnchor="14.0" />
      <ImageView fx:id="imageView" fitHeight="150.0" fitWidth="200.0" layoutX="14.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" />
    </children>
</AnchorPane>
